{
  "name": "webpack-conf-cli",
  "description": "webpack config generate cli",
  "version": "1.0.8",
  "author": "wangzhonghuan",
  "bin": {
    "wpconf": "./bin/run"
  },
  "bugs": "https://github.com/wzhkobe2408/webpack-conf-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/fs-extra": "^8.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "escodegen": "^1.11.1",
    "esprima": "^4.0.1",
    "estraverse": "^4.2.0",
    "fs-extra": "^8.1.0",
    "javascript-stringify": "^2.0.0",
    "tslib": "^1",
    "webpack-blocks": "^2.0.1",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/escodegen": "^0.0.6",
    "@types/esprima": "^4.0.2",
    "@types/estraverse": "^0.0.6",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/webpack-merge": "^4.1.5",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/wzhkobe2408/webpack-conf-cli",
  "keywords": [
    "oclif",
    "webpack",
    "config",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wpconf",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/wzhkobe2408/webpack-conf-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
